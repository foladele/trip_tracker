<h1>Edit:</h1>

<%= form_for @address do |f| %>

	<%= f.label :street %>
	<%= f.text_field :street %>

	<%= f.label :city %>
	<%= f.text_field :city %>

	<%= f.label :state %>
	<%= f.text_field :state %>

	<%= f.label :zip %>
	<%= f.text_field :zip %>

	<%= f.label :country %>
	<%= f.text_field :country %>

	<%= f.submit %>

<%end%>